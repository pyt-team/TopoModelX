        "y_true = np.zeros((34, 2))\n",
        "y_true[:, 0] = y\n",
        "y_true[:, 1] = 1 - y\n",
        "y_test = y_true[:4]\n",
        "y_train = y_true[-30:]\n",
        "\n",
        "y_train = torch.from_numpy(y_train)\n",
