      "execution_count": null,
      "metadata": {
        "id": "Ub-GWaMm0pEl"
      },
      "outputs": [],
      "source": [
        "y = np.array(\n",
        "    [\n",
        "        1,\n",
        "        1,\n",
        "        1,\n",
        "        1,\n",
        "        1,\n",
        "        1,\n",
